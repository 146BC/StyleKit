{
    "@mainFont": "HelveticaNeue-Bold:20.0",
    "@secondaryFont": "HelveticaNeue-Medium:13.0",
    "@primaryColor": "#cd0073",
    "@secondaryColor": "#8000c6",
    "@transparencyValue": 90,
    "StyleKitDemo.SKNavigationBar": {
        "tintColor": "@secondaryColor",
        "titleTextAttributes": {
            "NSColor": "@primaryColor",
            "NSFont": "@mainFont"
        }
    },
    "UITableView": {
        "UITableViewCell": {
            "UILabel": {
                "font": "@secondaryFont",
                "color": "#000"
            }
        }
    },
    "UITableViewCell": {
        "UILabel": {
            "font": "@secondaryFont",
            "color": "#000"
        }
    },
    "StyleKitDemo.SKButton": {
        "backgroundColor:normal": "@primaryColor",
        "backgroundColor:highlighted": "@secondaryColor",
        "titleColor:normal": "#fff",
        "titleColor:highlighted": "#fff",
        "font": "HelveticaNeue-Light:13.0",
        "transparency": "@transparencyValue"
    },
    "UIButton": {
        "font": "@secondaryFont",
        "titleColor:normal": "#000",
        "titleColor:highlighted": "#555"
    },
    "StyleKitDemo.SKLabel": {
        "attributes": {
            "NSKern": 3,
            "NSColor": "#f1c40f",
            "NSBackgroundColor": "#2c3e50",
            "NSFont": "Menlo:17",
            "NSParagraphStyle": {
                "alignment": "left",
                "lineSpacing": 5,
                "lineBreakMode": "byCharWrapping",
                "headIndent": 12
            }
        }
    },
    "UILabel": {
        "font": "HelveticaNeue-Bold:15.0",
        "color": "#8000c6"
    },
    "UIScrollView": {
        "UILabel": {
            "font": "HelveticaNeue-Light:15.0",
            "color": "#000"
        },
        "StyleKitDemo.SKView": {
            "StyleKitDemo.SKLabel": {
                "font": "HelveticaNeue-Bold:15.0",
                "color": "@primaryColor",
                "padding": [0,30,0,0],
                "backgroundColor": "#eee"
            }
        }
    },
    "UITextField": {
        "font": "HelveticaNeue-Light:15.0",
        "textColor": "#000",
        "UILabel": {
            "font": "HelveticaNeue-Medium:15.0",
            "color": "#000"
        }
    },
    "StyleKitDemo.SKTextField": {
        "textColor": "@primaryColor",
        "UILabel": {
            "color": "@primaryColor"
        }
    },
    "UISegmentedControl": {
        "tintColor": "@primaryColor",
        "titleTextAttributes:normal": {
            "NSColor": "#999",
            "NSFont": "HelveticaNeue-Light:15.0"
        },
        "titleTextAttributes:selected": {
            "NSColor": "#000",
            "NSFont": "HelveticaNeue-Medium:15.0"
        }
    },
    "UIActivityIndicatorView": {
        "color": "@primaryColor"
    }
}
