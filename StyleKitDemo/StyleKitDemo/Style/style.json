{
    "@mainFont": "HelveticaNeue-Bold:20.0",
    "@primaryColor": "#000000",
    "@secondaryColor": "#000FFF",
    "@transperancyValue": 90,
    "StyleKitDemo.SKNavigationBar": {
        "tintColor": "@secondaryColor",
        "titleTextAttributes": {
            "NSColor": "#99000000",
            "NSFont": "@mainFont"
        }
    },
    "UITableView": {
        "UITableViewCell": {
            "UILabel": {
                "font": "HelveticaNeue-Medium:13.0",
                "color": "#000"
            }
        }
    },
    "UITableViewCell": {
        "UILabel": {
            "font": "HelveticaNeue-Medium:13.0",
            "color": "#000"
        }
    },
    "StyleKitDemo.SKButton": {
        "backgroundColor:normal": "@primaryColor",
        "backgroundColor:highlighted": "#8000c6",
        "titleColor:normal": "#fff",
        "titleColor:highlighted": "#fff",
        "font": "HelveticaNeue-Light:13.0",
        "transparency":"@transperancyValue"
    },
    "UIButton": {
        "font": "HelveticaNeue-Medium:13.0",
        "titleColor:normal": "#000",
        "titleColor:highlighted": "#555"
    },
    "StyleKitDemo.SKLabel": {
        "font": "HelveticaNeue-Light:17.0",
        "color": "#cd0073"
    },
    "UILabel": {
        "font": "HelveticaNeue-Bold:15.0",
        "color": "#8000c6"
    },
    "UIScrollView": {
        "UILabel": {
            "font": "HelveticaNeue-Light:15.0",
            "color": "#000"
        },
        "StyleKitDemo.SKView": {
            "StyleKitDemo.SKLabel": {
                "font": "HelveticaNeue-Bold:15.0",
                "color": "#cd0073",
                "padding": [0,30,0,0],
                "backgroundColor": "#eee"
            }
        }
    },
    "UITextField": {
        "font": "HelveticaNeue-Light:15.0",
        "textColor": "#000",
        "UILabel": {
            "font": "HelveticaNeue-Medium:15.0",
            "color": "#000"
        }
    },
    "StyleKitDemo.SKTextField": {
        "textColor": "#cd0073",
        "UILabel": {
            "color": "#cd0073"
        }
    },
    "UISegmentedControl": {
        "tintColor": "#cd0073",
        "titleTextAttributes:normal": {
            "NSColor": "#999",
            "NSFont": "HelveticaNeue-Light:15.0"
        },
        "titleTextAttributes:selected": {
            "NSColor": "#000",
            "NSFont": "HelveticaNeue-Medium:15.0"
        }
    },
    "UIActivityIndicatorView": {
        "color": "#cd0073"
    }
}
